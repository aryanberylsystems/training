<p id="notice"><%= notice %></p>

<h1>Customer Mobiles</h1>

<table>
  <thead>
    <tr>
      <th>Buyer</th>
      <th>Seller</th>
      <th>Invoice</th>
      <th>Mobile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @customer_mobiles.each do |customer_mobile| %>
      <tr>
        <td><%= customer_mobile.buyer %></td>
        <td><%= customer_mobile.seller %></td>
        <td><%= customer_mobile.invoice %></td>
        <td><%= customer_mobile.mobile %></td>
        <td><%= link_to 'Show', customer_mobile %></td>
        <td><%= link_to 'Edit', edit_customer_mobile_path(customer_mobile) %></td>
        <td><%= link_to 'Destroy', customer_mobile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Customer Mobile', new_customer_mobile_path %>

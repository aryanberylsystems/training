<p id="notice"><%= notice %></p>
<h1>List of items</h1>
<% @products.each do |product| %>
<div class="entry">
<%= image_tag(product.image_url) %>
<h3><%= product.title %></h3>
<div class="desc">
<%= sanitize(product.description) %>
</div>
<div class="price_line">
<span class="price">&#8377;<%= product.price %></span>
</div>
</div>
<% end %>